[alias]
  addf = "!git ls-files -m -o --exclude-standard | fzf --print0 -m | xargs -0 -t -o git add"
  rbr = "!f() { git pull --rebase=interactive origin $1; }; f"
  logs = "!git log --oneline | head -n 5"
[pull]
	rebase = false
[oh-my-zsh]
	hide-status = 1
	hide-dirty = 1
	hide-status = 0
	hide-dirty = 0
	hide-status = 0
	hide-dirty = 0
[core]
	editor = nvim
[diff]
	tool = nvimdiff
[difftool]
	prompt = false
[rebase]
	autoSquash = true
